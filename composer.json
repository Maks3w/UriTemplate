{
    "name": "fr3d/uri-template",
    "type": "library",
    "description": "PHP URI Template (RFC 6570) supports both expansion & extraction. Fork of rize/uri-template but with RFC3986 fixes",
    "keywords": ["URI", "Template", "RFC 6570"],
    "license": "MIT",
    "require": {
        "php": ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0.0"
    },
    "replace": {
        "rize/uri-template": "*"
    },
    "autoload": {
        "psr-0": {
            "Rize\\UriTemplate\\": "src/"
        }
    }
}
